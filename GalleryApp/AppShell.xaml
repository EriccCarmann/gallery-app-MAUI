<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="GalleryApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:GalleryApp.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="GalleryApp">
    <TabBar>
        <Tab Title="New Photos">
            <ShellContent ContentTemplate="{DataTemplate local:NewPhotosPage}" />
        </Tab>
        <Tab Title="Saved Photos">
            <ShellContent ContentTemplate="{DataTemplate local:SavedPhotosPage}" />
        </Tab>
    </TabBar>
</Shell>
